<?xml version="1.0" encoding="UTF-8"?>
<monframework-config>
	<actions>
		<action>
			<action-name>fr.afcepf.atod26.framework.servlets.ActionUn
			</action-name>
			<url-pattern>actionun</url-pattern>
			<form-name>ActionUnForm</form-name>
			<from-view>test.jsp</from-view>
		</action>
		<action>
			<action-name>fr.afcepf.atod26.framework.servlets.ActionDeux
			</action-name>
			<url-pattern>actiondeux</url-pattern>
			<form-name>ActionDeuxForm</form-name>
			<from-view>actiondeux.jsp</from-view>
		</action>
	</actions>
	<forms>
		<form>
			<form-name>ActionUnForm</form-name>
			<form-class>fr.afcepf.atod26.framework.formbeans.ActionUnForm
			</form-class>
		</form>
		<form>
			<form-name>ActionDeuxForm</form-name>
			<form-class>fr.afcepf.atod26.framework.formbeans.ActionDeuxForm
			</form-class>
		</form>
	</forms>
	<beans>
		<bean id="factoryImpl" class="fr.afcepf.atod26.framework.impl.FactoryImpl" />
		<bean id="factoryXMLConfig" class="fr.afcepf.atod26.framework.impl.FactoryXMLConfig">
			<property name="config" ref="factoryImpl" />
		</bean>
	</beans>
</monframework-config>